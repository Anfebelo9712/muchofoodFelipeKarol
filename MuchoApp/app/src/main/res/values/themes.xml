<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MuchoApp" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/yellow_light</item>
        <item name="colorPrimaryDark">@color/yellow_dark</item>
        <item name="colorAccent">@color/blanco</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="Theme.MuchoApp.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.MuchoApp.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
    <style name="Theme.MuchoApp.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>